			end)
		end
		if gui.pick_node(gui.get_node("group_close_free"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_group_free")) then
			currentLevel.platform.stopGame = false
			gui.set_enabled(gui.get_node("popup_group_free"), false)
			gui.set_enabled(gui.get_node("overlay"), false)
		end
		if gui.pick_node(gui.get_node("group_recharge_free"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_group_free")) then
			currentLevel.platform.stopGame = false
