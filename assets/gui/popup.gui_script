function setTxtForAll(self)
	local lgConfPopupLatin = sys.load(ryoku_path)
	-- ZONE
	gui.set_text(gui.get_node("header_zone_free"), languages[lgConfPopupLatin.game_language].header_zone_popup)
	gui.set_text(gui.get_node("header_zone_pay"), languages[lgConfPopupLatin.game_language].header_zone_popup)

	gui.set_text(gui.get_node("get_zone_free"), languages[lgConfPopupLatin.game_language].get_zone_free)
	gui.set_text(gui.get_node("get_zone_pay"), languages[lgConfPopupLatin.game_language].get_zone_pay)

	gui.set_text(gui.get_node("offer_zone_free_gratuit"), languages[lgConfPopupLatin.game_language].label_market_free_gratuit)
	gui.set_text(gui.get_node("offer_zone_pay_facile"), languages[lgConfPopupLatin.game_language].label_market_pay_facile)

	gui.set_text(gui.get_node("offer_zone_free_unique"), languages[lgConfPopupLatin.game_language].label_market_free_unique)
	gui.set_text(gui.get_node("offer_zone_pay_jouer"), languages[lgConfPopupLatin.game_language].label_market_pay_jouer)

	gui.set_text(gui.get_node("txt_recharge_free_zone"), languages[lgConfPopupLatin.game_language].label_button_recharge)
	gui.set_text(gui.get_node("txt_recharge_pay_zone"), languages[lgConfPopupLatin.game_language].label_button_recharge)

