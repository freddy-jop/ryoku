end

function on_message(self, message_id, message, sender)
	if message_id == hash("zone_popup_pay") then
		managePopupLg(self)
		gui.set_enabled(gui.get_node("overlay"), true)
		gui.set_enabled(gui.get_node("popup_adjacent_pay"), true)
	end
	if message_id == hash("target_popup_pay") then
		managePopupLg(self)
		gui.set_enabled(gui.get_node("overlay"), true)
		gui.set_enabled(gui.get_node("popup_target_pay"), true)
	end
	if message_id == hash("group_popup_pay") then
		managePopupLg(self)
		gui.set_enabled(gui.get_node("overlay"), true)
		gui.set_enabled(gui.get_node("popup_group_pay"), true)
	end
	if message_id == hash("sudoku_popup_pay") then
		managePopupLg(self)
		gui.set_enabled(gui.get_node("overlay"), true)
		gui.set_enabled(gui.get_node("popup_sudoku_pay"), true)
	end
	if message_id == hash("display_info") then
		gui.set_enabled(gui.get_node("overlay_info"), true)
		gui.set_enabled(gui.get_node("popup_info_powerup"), true)
	end
end

function on_input(self, action_id, action)
	if action_id == hash("touch") and action.pressed then
		-- popup powerup zone
		if gui.pick_node(gui.get_node("adjacent_close_pay"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_adjacent_pay")) then
			gui.set_enabled(gui.get_node("popup_adjacent_pay"), false)
			timer.delay(0.5, false, function() 
				managePopupLg(self)
				gui.set_enabled(gui.get_node("popup_adjacent_free"), true)
			end)
		end
		if gui.pick_node(gui.get_node("adjacent_close_free"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_adjacent_free")) then
			currentLevel.platform.stopGame = false
			gui.set_enabled(gui.get_node("popup_adjacent_free"), false)
			gui.set_enabled(gui.get_node("overlay"), false)
		end
		if gui.pick_node(gui.get_node("adjacent_recharge_free"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_adjacent_free")) then
			currentLevel.platform.stopGame = false
			local rechargeZone = sys.load(ryoku_path)
			rechargeZone.adjacent = rechargeZone.adjacent + 1
			if sys.save(ryoku_path, rechargeZone) then
				msg.post("/powerup#powerup", "increment_zone")
			end
			gui.set_enabled(gui.get_node("popup_adjacent_free"), false)
			gui.set_enabled(gui.get_node("overlay"), false)
		end
		-- popup powerup target
		if gui.pick_node(gui.get_node("target_close_pay"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_target_pay")) then
			gui.set_enabled(gui.get_node("popup_target_pay"), false)
			timer.delay(0.5, false, function() 
				managePopupLg(self)
				gui.set_enabled(gui.get_node("popup_target_free"), true)
			end)
		end
		if gui.pick_node(gui.get_node("target_close_free"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_target_free")) then
			currentLevel.platform.stopGame = false
			gui.set_enabled(gui.get_node("popup_target_free"), false)
			gui.set_enabled(gui.get_node("overlay"), false)
		end
		if gui.pick_node(gui.get_node("target_recharge_free"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_target_free")) then
			currentLevel.platform.stopGame = false
			local rechargeTarget = sys.load(ryoku_path)
			rechargeTarget.target = rechargeTarget.target + 1
			if sys.save(ryoku_path, rechargeTarget) then
				msg.post("/powerup#powerup", "increment_target")
			end
			gui.set_enabled(gui.get_node("popup_target_free"), false)
			gui.set_enabled(gui.get_node("overlay"), false)
		end
		-- popup powerup group
		if gui.pick_node(gui.get_node("group_close_pay"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_group_pay")) then
			gui.set_enabled(gui.get_node("popup_group_pay"), false)
			timer.delay(0.5, false, function() 
				managePopupLg(self)
				gui.set_enabled(gui.get_node("popup_group_free"), true)
			end)
		end
		if gui.pick_node(gui.get_node("group_close_free"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_group_free")) then
			currentLevel.platform.stopGame = false
			gui.set_enabled(gui.get_node("popup_group_free"), false)
			gui.set_enabled(gui.get_node("overlay"), false)
		end
		if gui.pick_node(gui.get_node("group_recharge_free"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_group_free")) then
			currentLevel.platform.stopGame = false
			local rechargeGroup = sys.load(ryoku_path)
			rechargeGroup.group = rechargeGroup.group + 1
			if sys.save(ryoku_path, rechargeGroup) then
				msg.post("/powerup#powerup", "increment_group")
			end
			gui.set_enabled(gui.get_node("popup_group_free"), false)
			gui.set_enabled(gui.get_node("overlay"), false)
		end

		-- popup powerup sudoku
		if gui.pick_node(gui.get_node("sudoku_close_pay"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_sudoku_pay")) then
			gui.set_enabled(gui.get_node("popup_sudoku_pay"), false)
			timer.delay(0.5, false, function() 
				managePopupLg(self)
				gui.set_enabled(gui.get_node("popup_sudoku_free"), true)
			end)
		end
		if gui.pick_node(gui.get_node("sudoku_close_free"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_sudoku_free")) then
			currentLevel.platform.stopGame = false
			gui.set_enabled(gui.get_node("popup_sudoku_free"), false)
			gui.set_enabled(gui.get_node("overlay"), false)
		end
		if gui.pick_node(gui.get_node("sudoku_recharge_free"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_sudoku_free")) then
			currentLevel.platform.stopGame = false
			local rechargeSudoku = sys.load(ryoku_path)
			rechargeSudoku.sudoku = rechargeSudoku.sudoku + 1
			if sys.save(ryoku_path, rechargeSudoku) then
				msg.post("/powerup#powerup", "increment_sudoku")
			end
			gui.set_enabled(gui.get_node("popup_sudoku_free"), false)
			gui.set_enabled(gui.get_node("overlay"), false)
		end
		if gui.pick_node(gui.get_node("info_button"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_info_powerup")) then
			currentLevel.platform.stopGame = false
			gui.set_enabled(gui.get_node("popup_info_powerup"), false)
			gui.set_enabled(gui.get_node("overlay_info"), false)
		end
	end
end

