function on_message(self, message_id, message, sender)
	if message_id == hash("zone_popup_pay") then
		gui.set_enabled(gui.get_node("overlay"), true)
		gui.set_enabled(gui.get_node("popup_adjacent_pay"), true)
	end
	if message_id == hash("target_popup_pay") then
		gui.set_enabled(gui.get_node("overlay"), true)
		gui.set_enabled(gui.get_node("popup_target_pay"), true)
	end
	if message_id == hash("group_popup_pay") then
		gui.set_enabled(gui.get_node("overlay"), true)
		gui.set_enabled(gui.get_node("popup_group_pay"), true)
	end
	-- Add message-handling code here
	-- Learn more: https://defold.com/manuals/message-passing/
	-- Remove this function if not needed
end

function on_input(self, action_id, action)
	if action_id == hash("touch") and action.pressed then
		-- popup powerup zone
		if gui.pick_node(gui.get_node("adjacent_close_pay"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_adjacent_pay")) then
			gui.set_enabled(gui.get_node("popup_adjacent_pay"), false)
			timer.delay(0.5, false, function() 
				gui.set_enabled(gui.get_node("popup_adjacent_free"), true)
			end)
		end
		if gui.pick_node(gui.get_node("adjacent_close_free"), action.x, action.y) and gui.is_enabled(gui.get_node("popup_adjacent_free")) then
			currentLevel.platform.stopGame = false
			gui.set_enabled(gui.get_node("popup_adjacent_free"), false)
			gui.set_enabled(gui.get_node("overlay"), false)
		end
