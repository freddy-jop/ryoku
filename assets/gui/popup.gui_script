function managePopupLg(self)
	local manageConfigPopup = sys.load(ryoku_path)
	if manageConfigPopup.game_language == "ru" or manageConfigPopup.game_language == "be" then
		setRuFont(self)
	end
	if manageConfigPopup.game_language == "ar" then
		setArbFont(self)
	end
	if manageConfigPopup.game_language == "ja" then
		setJaFont(self)
	end
	if manageConfigPopup.game_language == "ko" then
		setKrFont(self)
	end
	if manageConfigPopup.game_language == "vi" then
		setViFont(self)
	end
	if manageConfigPopup.game_language == "fr" or 
	manageConfigPopup.game_language == "en" or 
	manageConfigPopup.game_language == "es" or 
	manageConfigPopup.game_language == "de" or 
	manageConfigPopup.game_language == "pt" or 
	manageConfigPopup.game_language == "pl" or 
	manageConfigPopup.game_language == "nl" or 
	manageConfigPopup.game_language == "tr" or 
	manageConfigPopup.game_language == "it" or 
	manageConfigPopup.game_language == "id" then
		setLatineFont(self)
	end
	if manageConfigPopup.game_language == "ar" then
		setTxtArabic(self)
		setInformationReperesArabic(self)
	else
		setTxtForAll(self)
		setInformationReperes(self)
	end
end

function init(self)
	msg.post(".", "acquire_input_focus")
	managePopupLg(self)
	gui.set_render_order(0)
end

